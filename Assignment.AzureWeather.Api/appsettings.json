{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WeatherDb": "<connection-string>"
  },
  "FrontUrl": "http://localhost:3000",
  "WeatherService": {
    "Units": "metric",
    "ApiKey": "<api_key>",
    "ServiceUrl": "https://api.openweathermap.org"
  },
  "WeatherWorker": {
    "Locations": ["Stockholm, SE", "Gothenburg, SE", "Oslo, NO", "Bergen, NO", "Copenhagen, DK", "Frederikssund, DK"],
    "IntervalSeconds": 60
  }
}
